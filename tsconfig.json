{
  "compileOnSave": true,
  "compilerOptions": {
    "downlevelIteration": true,
    "lib": [ "es6" ],
    "newLine": "lf",
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./out",
    "preserveConstEnums": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "rootDir": "src",
    "sourceMap": true,
    "strict": true,
  },
}
