    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chalk-lang/ts-compiler.git"
  },
  "author": "Jozef Miku≈°inec <mik.jozef@gmail.com> (mikusinec.com)",
  "bugs": {
    "url": "https://github.com/chalk-lang/ts-compiler/issues"
  },
  "homepage": "https://github.com/chalk-lang/ts-compiler#readme",
  "devDependencies": {
    "@types/jest": "^26.0.7",
    "@types/node": "^12.7.11",
    "jest": "^26.1.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {},
  "jest": {
    "rootDir": "out/",
    "testEnvironment": "jest-environment-node"
  },
  "type": "module"
